{
  "name": "Eth0.se URL shortener",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Shorten urls using http://eth0.se",
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "popup.html",
    "default_title": "Shorten url using eth0.se"
  },
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  },
  "permissions": [
    "http://app.eth0.se/",
    "http://eth0.se/",
    "tabs",
    "clipboardWrite"

  ]
}